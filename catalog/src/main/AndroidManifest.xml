<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.PureLink.Catalog.cluelin">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:icon="@drawable/catalog_2016_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".PDFViewActivity_"/>

        <activity android:name="com.PureLink.Catalog.cluelin.MainActivity" />
        <activity android:name="com.PureLink.Catalog.cluelin.LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.PureLink.Catalog.cluelin.IndexActivity"></activity>
    </application>

</manifest>